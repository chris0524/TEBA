	<queryString><![CDATA[SELECT (SELECT ORGANSNAME FROM SYSCA_ORGAN B WHERE B.ORGANID=A.ENTERORG) ORGANSNAME,
       (SELECT E.SIGNDESC||SUBSTR(SUBSTR(C.SIGNNO,8),1,4)||'-'||SUBSTR(SUBSTR(C.SIGNNO,8),5,4) N_SNO
	  FROM UNTLA_LAND C, SYSCA_SIGN E
	 WHERE C.ENTERORG=A.ENTERORG
	   AND C.PROPERTYNO=A.PROPERTYNO
	   AND C.SERIALNO=A.SERIALNO
	   AND C.OWNERSHIP=A.OWNERSHIP
	   AND C.DATASTATE='1'
	   AND SUBSTR(C.SIGNNO, 1, 7)=E.SIGNNO ) N_SIGNO,
       (SELECT CODENAME 
          FROM SYSCA_CODE G
         WHERE G.CODEKINDID='TKD'
           AND G.CODEID=A.TAXKIND) T_TAXKIND,
       TO_CHAR(A.REMITAREA) REMITAREA,A.TAXAMOUNT,'' T_DOOR, 'A' T_LB
  FROM UNTLA_TAX A
UNION
SELECT (SELECT ORGANSNAME FROM SYSCA_ORGAN I WHERE I.ORGANID=H.ENTERORG) ORGANSNAME,
       (SELECT F.SIGNDESC||SUBSTR(SUBSTR(D.SIGNNO,8),1,5)||'-'||SUBSTR(SUBSTR(D.SIGNNO,8),6,3) N_SNO1
	  FROM UNTBU_BUILDING D, SYSCA_SIGN F
	 WHERE D.ENTERORG=H.ENTERORG
	   AND D.PROPERTYNO=H.PROPERTYNO
	   AND D.SERIALNO=H.SERIALNO
	   AND D.OWNERSHIP=H.OWNERSHIP
	   AND D.DATASTATE='1'
	   AND SUBSTR(D.SIGNNO, 1, 7)=F.SIGNNO ) N_SIGNO,
       (SELECT CODENAME FROM 
         SYSCA_CODE J
         WHERE J.CODEKINDID='TKD'
           AND J.CODEID=H.TAXKIND) T_TAXKIND,
       NULL REMITAREA,H.TAXAMOUNT,
       (SELECT K.ADDRNAME||L.ADDRNAME||M.ADDRNAME||J.DOORPLATE4 DOORPLATE
	  FROM UNTBU_BUILDING J, SYSCA_ADDR K, SYSCA_ADDR L, SYSCA_ADDR M
	 WHERE J.ENTERORG=H.ENTERORG
	   AND J.PROPERTYNO=H.PROPERTYNO
	   AND J.SERIALNO=H.SERIALNO
	   AND J.OWNERSHIP=H.OWNERSHIP
	   AND J.DATASTATE='1'
	   AND J.DOORPLATE1=K.ADDRID
	   AND K.ADDRKIND='1'
	   AND J.DOORPLATE2=L.ADDRID
	   AND L.ADDRKIND='2'
	   AND J.DOORPLATE3=M.ADDRID
	   AND M.ADDRKIND='3')  T_DOOR, 'B' T_LB
  FROM UNTBU_TAX H]]></queryString>

